{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "lib": ["dom", "esnext"],
    "types": [ "jest", "@testing-library/jest-dom","@types/react-router","@types/react-router-dom","websocket"],
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*","App.tsx","main.tsx","declaration.d.ts","test/**/*"],
  "exclude": ["node_modules", "dist", "test"]
}
